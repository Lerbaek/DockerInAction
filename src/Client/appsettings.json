{
  "AllowedHosts": "*",
  "ClientOptions": {
    "Path" : "http://server:8080/PaymentExecution"
  },
  "Logging": {
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "TimestampFormat": "[yyyy-MM-ddTHH:mm:ss.fffZ] ",
        "UseUtcTimestamp": true
      }
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RabbitMq": {
    "Host": "rabbitmq",
    "User": "guest",
    "Pass": "guest"
  }
}
